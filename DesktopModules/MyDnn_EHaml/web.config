  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="SendSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://www.sms.gostara.net/post/send.asmx"
                binding="basicHttpBinding" bindingConfiguration="SendSoap" contract="ServiceReference1.SendSoap"
                name="SendSoap" />
    </client>
  </system.serviceModel>
